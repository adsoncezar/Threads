<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adsoncezar.theads">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CadastroUsuarioActivity"
            android:label="Cadastrar Usuário" />
        <activity android:name=".MainActivity"
            android:label="Controle de Bens">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThreadActivity" />
        <activity
            android:name=".LocalidadeActivity"
            android:label="Localidades"
            />
        <activity
            android:name=".BemActivity"
            android:label="Patrimônio" />
        <activity
            android:name=".CadastrarLocalidadeActivity"
            android:label="@string/title_activity_cadastrar_localidade"></activity>
        <activity
            android:name=".CadastrarBemActivity"
            android:label="Cadastrar Bens" />

        <service
            android:name=".MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name=".MyMessageService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>